	<parameter name="TAXESLOGIC" isForPrompting="false" class="com.openbravo.pos.sales.TaxesLogic"/>
	<queryString><![CDATA[SELECT P.ID, P.REFERENCE, P.CODE, P.NAME, P.PRICEBUY, P.PRICESELL, 
(P.PRICESELL * (1 + T1.RATE)) AS PRICESELLTAX, T1.NAME AS TAXNAME, T1.RATE AS TAXRATE,
P.CATEGORY, C.NAME AS CATEGORYNAME 
FROM PRODUCTS P ,CATEGORIES C, TAXES T1 
WHERE  P.CATEGORY = C.ID   AND P.TAXCAT = T1.CATEGORY AND (P.TAXCAT2 IS NULL OR P.TAXCAT2='') AND ( P.TAXCAT3 IS NULL OR P.TAXCAT3 ='')
UNION ALL

SELECT P.ID, P.REFERENCE, P.CODE, P.NAME, P.PRICEBUY, P.PRICESELL, 
(P.PRICESELL * (1 + (T1.RATE+T2.RATE))) AS PRICESELLTAX, T1.NAME AS TAXNAME, (T1.RATE+T2.RATE) AS TAXRATE,
P.CATEGORY, C.NAME AS CATEGORYNAME 
FROM PRODUCTS P ,CATEGORIES C, TAXES T1 , TAXES T2 
WHERE  P.CATEGORY = C.ID   AND P.TAXCAT = T1.CATEGORY AND P.TAXCAT2=T2.CATEGORY AND( P.TAXCAT3 IS NULL OR P.TAXCAT3 ='')
UNION ALL

SELECT P.ID, P.REFERENCE, P.CODE, P.NAME, P.PRICEBUY, P.PRICESELL, 
(P.PRICESELL * (1 + (T1.RATE+T2.RATE+T3.RATE))) AS PRICESELLTAX, T1.NAME AS TAXNAME, (T1.RATE+T2.RATE+T3.RATE) AS TAXRATE,
P.CATEGORY, C.NAME AS CATEGORYNAME 
FROM PRODUCTS P ,CATEGORIES C, TAXES T1 , TAXES T2 , TAXES T3
WHERE  P.CATEGORY = C.ID   AND P.TAXCAT = T1.CATEGORY AND P.TAXCAT2=T2.CATEGORY AND P.TAXCAT3 = T3.CATEGORY
ORDER BY 11, 4]]></queryString>

