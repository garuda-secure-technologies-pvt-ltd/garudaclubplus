    private void loadData(){
      try {
            pjEditorTableModel = AJTableModel.loadData(app, (Date) Formats.DATE.parseValue(jTextField1.getText()), (Date) Formats.DATE.parseValue(jTextField2.getText()));
            jTable1.setModel(pjEditorTableModel.getTableModel());
        } catch (BasicException ex) {
           ex.printStackTrace();
        }
    }

   
