    public void init(AppView app) throws BeanFactoryException {
        m_App = app;
        dlsales = (DataLogicSales) m_App.getBean("com.openbravo.pos.forms.DataLogicSalesCreate");
        dlfac = (DataLogicFacilities) app.getBean("com.openbravo.pos.clubmang.DataLogicFacilitiesCreate");
        warehouse = new ComboBoxValModel();
        consol = new ComboBoxValModel();
        wareho = new HashMap<String, String>();
        loc = new HashMap<String, String>();
        parent = new ComboBoxValModel();

    }

    public Object getBean() {
        return this;
    }

    private void load() throws BasicException {
        jButton2.setVisible(false);
        jTextField1.setText(null);
        jTextField1.setEnabled(true);
        jRadioButton1.setSelected(false);
        jRadioButton2.setSelected(false);
        jRadioButton3.setSelected(false);
        jRadioButton4.setSelected(false);
        warehouselist = new ArrayList();
        wmodel = new WarehouseListModel(warehouselist);
        jList1.setModel(wmodel);
        m_sentParent = dlsales.getParentLocationsList();
        parent = new ComboBoxValModel(m_sentParent);
        jComboBox3.setModel(parent);
        jComboBox1.setModel(warehouse);
        List<LocationInfo> linfo = dlsales.getAllLocationsList();
        for (LocationInfo l : linfo) {
            wareho.put(l.getName(), l.getID());
            loc.put(l.getID(), l.getName());
        }
        List<MultipleWarehouse> list = dlsales.getConsolidateReports();
        consol = new ComboBoxValModel(list);
