private void jButton6ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton6ActionPerformed
  String URL=url.getText(); 
   String USER=username.getText();
    String urlref=urlref_combo.getSelectedItem().toString();
    String sender_id=senderid.getText();
    String API_KEY=jTextField2.getText();
   int x=JOptionPane.showConfirmDialog(jPanel3, "Do you want to save");
   if(x==JOptionPane.YES_OPTION)
   {
        try {
            new PreparedSentence(m_app.getSession(), "INSERT INTO smsurl_table(ID, URL, USERNAME, PASSWORD, ACTIVE,SENDERID,URLREF,API_KEY) VALUES (?,?,?,?,?,?,?,?) ", new SerializerWriteBasic(new Datas[]{Datas.STRING, Datas.STRING, Datas.STRING, Datas.STRING, Datas.INT, Datas.STRING, Datas.STRING,Datas.STRING})).exec(new Object[]{UUID.randomUUID().toString(),URL, USER, m_sPassword, 1,sender_id,urlref,API_KEY});
        } catch (BasicException ex) {
            Logger.getLogger(SmsSendernew.class.getName()).log(Level.SEVERE, null, ex);
        }
        jPanel3.setVisible(false);
   }
   else
   {
       url.setText("");
       username.setText("");
       m_sPassword="";
       jPanel3.setVisible(false);
       
   }
   
}//GEN-LAST:event_jButton6ActionPerformed

