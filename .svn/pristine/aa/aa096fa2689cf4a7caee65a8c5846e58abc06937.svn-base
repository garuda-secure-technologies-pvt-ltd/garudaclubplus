private void jButton3ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton3ActionPerformed
    
    
    reset();
    jPanel2.setVisible(false); 
     jPanel3.setVisible(false);
    String sPassword;
    
    try {
            List<Object[]> obj=(List<Object[]>)new PreparedSentence(m_app.getSession(), "SELECT S.ID,S.URL,S.USERNAME,S.PASSWORD,S.ACTIVE,S.SENDERID,S.URLREF FROM SMSURL_TABLE S WHERE S.ACTIVE=TRUE AND S.URLREF='PASSCH_URL'",SerializerWriteString.INSTANCE, new SerializerReadBasic(new Datas[]{Datas.STRING,Datas.STRING,Datas.STRING,Datas.STRING,Datas.BOOLEAN,Datas.STRING,Datas.STRING})).list();
           for (Object[] url1 : obj) {
            //urlBean=(UrlBean)url;
            obj1=url1;
           }
        
        } catch (BasicException ex) {
            Logger.getLogger(SmsSendernew.class.getName()).log(Level.SEVERE, null, ex);
        }
    
        if(obj1!=null){ 
            
            
         sPassword=obj1[3].toString();
         String sNewPassword = Hashcypher.changePasswordforSMS(this,  sPassword);
         String passwd=new AltEncrypter("key").decrypt(sNewPassword);
         System.out.println(passwd);
         
         System.out.println(sNewPassword );
         String response= new SmsConnection(m_app).connectionPasswordChange( obj1,passwd);
          
          if(response.equalsIgnoreCase("Your Password is changed Successfully !!!"))
          {
             try {
                  new PreparedSentence(m_app.getSession(), "UPDATE smsurl_table SET PASSWORD=? WHERE PASSWORD=?", new SerializerWriteBasic(new Datas[]{Datas.STRING,Datas.STRING})).exec(new Object[]{sNewPassword,sPassword});
                } catch (BasicException ex) {
              Logger.getLogger(SmsSendernew.class.getName()).log(Level.SEVERE, null, ex);
                 }
           JOptionPane.showMessageDialog(this,response);
         }  
          else
          {
              JOptionPane.showMessageDialog(this,response);
          }
          
          
        }  
        else{
              JOptionPane.showMessageDialog(this,"Please contact administrator to change password..!!");
        }
}//GEN-LAST:event_jButton3ActionPerformed

private void jButton4ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton4ActionPerformed
        String mobileno=jTextField1.getText();
        Pattern pattern=Pattern.compile("^[789]\\d{9}$");
        Matcher matcher=pattern.matcher(mobileno);
        if(matcher.matches())
        {
        if(mobileno.length()<10 ||mobileno.length()>10){
            JOptionPane.showMessageDialog(this,"Please enter valid 10 digit mobile number");
            jTextField1.setText("");
           jTextField1.requestFocus(true);
        }
        else{
            connection=new SmsConnection(m_app);
        String response=  connection.connectionDelReport(mobileno);
        if(response==null)
        {
           JOptionPane.showMessageDialog(this,"No Active Url... Please Set url first...!!");  
        }
        else
        {
        
        //JOptionPane.showMessageDialog(this,response);
        generateReport( connection.getDelperlist());
        jPanel2.setVisible(false);
        }
        jPanel2.setVisible(false);
        }
        }
        else
        {
           JOptionPane.showMessageDialog(this,"Please enter valid 10 digit mobile number");
            jTextField1.setText("");
           jTextField1.requestFocus(true); 
        }
         
    // TODO add your handling code here:
}//GEN-LAST:event_jButton4ActionPerformed

public void generateReport(List<SmsConnection.DelRepBean> list)
{
     //List<SmsConnection.DelRepBean> list = new ArrayList<SmsConnection.DelRepBean>();
    Map reportparams = new HashMap();
      //list= new SmsConnection(m_app).getDelperlist();
     DataSourceProvider data1 = new DataSourceProvider(list);
     DataSourceForSMSDelReport dsfc = new DataSourceForSMSDelReport(list);
     data1.setDataSource(dsfc);
     reportparams.put("cname", m_app.getSession().getCompanyName());
     reportparams.put("caddress", m_app.getSession().getCompanyAddress());
     reportparams.put("reportType","SMS Delivery Report for");
    JasperPrint jp = JasperReportNew.runReport(m_app, "./reports/com/openbravo/reports/smsdeliveryreport.jrxml", reportparams, false, data1, true, null); 
    
    
}





