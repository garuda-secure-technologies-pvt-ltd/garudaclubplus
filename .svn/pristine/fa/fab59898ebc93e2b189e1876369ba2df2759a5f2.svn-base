     private boolean isAlpha(String s)
    {
       s = s.toUpperCase();
      for (int i = 0; i < s.length(); i ++)
      {
       int c = (int) s.charAt(i);
      if ((c < 65 || c > 90) && (c<47 || c>58) )
       return false;
      }
        return true;
     }

