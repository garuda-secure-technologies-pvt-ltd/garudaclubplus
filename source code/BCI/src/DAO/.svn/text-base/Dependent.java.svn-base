package DAO;

/**
 *
 * @author Ratan
 */
public class Dependent {

   private String name;
   private  String card;
   private byte[] fpdata;
   private String dno;

    public Dependent(String name, String card, byte[] fpdata,String dno) {
        this.name = name;
        this.card = card;
        this.fpdata = fpdata;
        this.dno=dno;
    }

    public String getDependentNo(){
      return dno;
    }

    public String getCard() {
        return card;
    }

    public byte[] getFpdata() {
        return fpdata;
    }

    public String getName() {
        return name;
    }

    @Override
    public String toString() {
        return dno+" - "+name;
    }

   

}
